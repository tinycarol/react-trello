{
  "name": "react-trello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "font-awesome": "^4.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "dotenv -e .env.production -- npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d build",
    "buildeploy": "npm run predeploy && npm run deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "dotenv-cli": "^3.1.0",
    "gh-pages": "^2.2.0"
  },
  "homepage": "http://tinycarol.github.io/react-trello"
}
